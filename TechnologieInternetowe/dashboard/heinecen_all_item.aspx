function buildDefQuery(ordArr){

    var qStart = orderByStMk;
	var qEnd = "</OrderBy>";
	var dQuery;
	

	var retArr = new Array();
	if(typeof ordArr == 'undefined'){retArr = defOrderBy;}else{retArr = ordArr;}
					
	if(retArr.length != 0){
		dQuery = qStart;
				
		for(var i=0;i<retArr.length;i++){
						
			dQuery += "<FieldRef Name='" + standShrpSpecMarks(retArr[i]['field']);
						
			if(typeof retArr[i]['sort'] == 'undefined' || retArr[i]['sort'] == '' || retArr[i]['sort'] == "asc"){
				dQuery += "' Ascending='True'";
			}else{dQuery += "' Ascending='False'";}
						
			dQuery += ' />';
		}
					
		dQuery += qEnd;
	}
	dQuery = "<Query>" + dQuery+ "</Query>";
	
	return dQuery;
}

