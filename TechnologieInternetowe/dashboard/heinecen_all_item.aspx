    function handleFetchDataSuccess(resultData) {
        var fileURL = $(resultData).find('#ExpRedirectURL').val();
        var fileID = $(resultData).find('#ExpFileID').val();
        var pageIndex = parseInt($(resultData).find('#ExpPageIndex').val());
        var pathToSearch = $(resultData).find('#ExpPathToSearchURL').val();
        var pageSize = $(resultData).find('#ExpPageSize').val();
        var pagesCount = parseInt($(resultData).find('#ExpPagesCount').val());

        if(pageIndex < pagesCount) {
            var status = ((pageIndex / pagesCount) * 100).toFixed(2);
            if (pageIndex > 0)
                $("#myExportPercent").html(status);
            pageIndex += 1;
            ExportData(pathToSearch, pageIndex, pagesCount, fileURL, fileID, pageSize);
        }
        else {
            $('#myExportLoader').hide();
            //$("#myExportPercent").html('100').delay(500);
            $("#myExportText").html("Done! Please download generated file...").delay(3000).fadeOut(1000);
            
            document.location.href = fileURL;
        }
    }

    /*
        Module: Export to CSV
        Description:
        Author: Pawel Krolczyk
        Last modification: Pawel Krolczyk
    */
    function ExportData(pathToSearch, pageIndex, pagesCount, fileURL, fileID, pageSize) {
        var pathToSearchTmp = decodeURIComponent(pathToSearch) + '&search_path=' + encodeURIComponent(pathToSearch) + '&page_index=' + pageIndex + '&pages_count=' + pagesCount + '&page_size=' + pageSize + '&file_url=' + fileURL + '&file_id=' + fileID;
        $.ajax({
            url: pathToSearchTmp,
            type: "POST",
            success: handleFetchDataSuccess,
        })
    }

function execQuery(gString,ord){
	
	var urlParams = parseUrl(); 
	var next;var query;var pageNr;
	var presPage;var prevPage;
	var ordQuery;

	//pobranie danych z url
	if(urlParams.length != 0){
		if(urlParams['nextStr'] && urlParams['nextStr'].length != 0){
			next = urlParams['nextStr'];			
		}
		if(urlParams['presPage'] && urlParams['presPage'].length != 0){
			presPage = urlParams['presPage'];			
		}
		if(urlParams['prevPage'] && urlParams['prevPage'].length != 0){
			prevPage = urlParams['prevPage'];			
		}		
		if(urlParams['pageNo'] && urlParams['pageNo'].length != 0){
			pageNr = urlParams['pageNo'];
		}
		if(urlParams['pageSrt'] && urlParams['pageSrt'].length != 0){
			if(sortGrdPrmSet == ''){
				sortGrdPrmSet = urlParams['pageSrt'];
			}
		}
		if(urlParams['pageGrp'] && urlParams['pageGrp'].length != 0){
			groupByField = urlParams['pageGrp'];
		}	
		if(urlParams['queryStr'] && urlParams['queryStr'].length != 0){
			query = urlParams['queryStr'];
		}
		if(typeof defRowNoChange != 'undefined' && defRowNoChange != ''){
			defRowNo = defRowNoChange;
		}
		else if(urlParams['numberOfRows'] && urlParams['numberOfRows'].length != 0){
			defRowNo = urlParams['numberOfRows'];
		}
	    
		//pobranie sortowania
		if(ord == false){
			query = defQuery;
		}
	    
	    if(typeof query != 'undefined' && query != ''){
	        ordQuery = query.replace("<Query>","").replace("</Query>","").trim();
	    }
	    else{
	        ordQuery = "";
	    }
		if(ordQuery.indexOf("<OrderBy") != -1){
		    ordQuery = ordQuery.substring(ordQuery.indexOf("<OrderBy"),ordQuery.indexOf("</OrderBy>")+10).trim();
		}
		else{
			ordQuery = '';
		}
		
		//parsowanie nowego zapyt
		var splGStr = new Array();
		if(gString.indexOf("&") != -1){
			splGStr = gString.split("&");
		}
		else{
			splGStr[0] = gString;
		}
		
		var newQuery = "<Where>";
		var conditions = [];
		
		//parsowanie kolejnych pol
		if(splGStr.length != 0){
			for(var z=0;z<splGStr.length;z++){
				
				//pobranie pola i wartosci z filtra
				splGStr[z] = splGStr[z].replace(queryStringPrefix,"");
				var fltFldName = splGStr[z].substring(0,splGStr[z].indexOf("=")).trim();
				var fltFldVal = Base64.decode(splGStr[z].substring(splGStr[z].indexOf("=")+1,splGStr[z].length).trim());
				
				//definicja domyslnego typu value + wykrywanie value na polu
				var defValType = "Text";
				var searchType = array_search(fltFldName,colNamesArray);
				if(searchType !== false){
					defValType = revSortType(colTypesArray[searchType]);
				}
				
				//zlozenie cond
				if(typeof fltFldName != 'undefined' && fltFldName != ''){
					if(fltFldVal.indexOf("[#toDate#]") == -1 && fltFldVal.indexOf("[#toNumber#]") == -1){				
						if(defValType == "Text"){
							conditions.push("<Contains><FieldRef Name='" + fltFldName + "' /><Value Type='" + defValType + "'>" + fltFldVal + "</Value></Contains>");
						}
						else if(defValType == "Number"){
							conditions.push("<Eq><FieldRef Name='" + fltFldName + "' /><Value Type='" + defValType + "'>" + fltFldVal + "</Value></Eq>");							
						}
						else if(defValType == "Integer"){
							conditions.push("<Eq><FieldRef Name='" + fltFldName + "' /><Value Type='" + defValType + "'>" + fltFldVal + "</Value></Eq>");							
						}						
						else if(defValType == "Counter"){
							conditions.push("<Eq><FieldRef Name='" + fltFldName + "' /><Value Type='" + defValType + "'>" + fltFldVal + "</Value></Eq>");
						}		
						else if(defValType == "DateTime"){
							conditions.push("<Eq><FieldRef Name='" + fltFldName + "' /><Value Type='" + defValType + "'>" + fltFldVal + "</Value></Eq>");
						}					
					}
					else{
					
						if(fltFldVal.indexOf("[#toDate#]") != -1){
							
							if(typeof fltFldVal.split("[#toDate#]")[0] != 'undefined' && fltFldVal.split("[#toDate#]")[0] != ''){							
								conditions.push("<Geq><FieldRef Name='" + fltFldName + "' /><Value IncludeTimeValue='False' Type='DateTime'>" + fltFldVal.split("[#toDate#]")[0] + "</Value></Geq>");
							}
							if(typeof fltFldVal.split("[#toDate#]")[1] != 'undefined' && fltFldVal.split("[#toDate#]")[1] != ''){
								conditions.push("<Leq><FieldRef Name='" + fltFldName + "' /><Value IncludeTimeValue='False' Type='DateTime'>" + fltFldVal.split("[#toDate#]")[1] + "</Value></Leq>");
							}
						}
						
						if(fltFldVal.indexOf("[#toNumber#]") != -1){
							if(typeof fltFldVal.split("[#toNumber#]")[0] != 'undefined' && fltFldVal.split("[#toNumber#]")[0] != ''){
								conditions.push("<Geq><FieldRef Name='" + fltFldName + "' /><Value Type='Number'>" + fltFldVal.split("[#toNumber#]")[0] + "</Value></Geq>");
							}
							if(typeof fltFldVal.split("[#toNumber#]")[1] != 'undefined' && fltFldVal.split("[#toNumber#]")[1] != ''){
								conditions.push("<Leq><FieldRef Name='" + fltFldName + "' /><Value Type='Number'>" + fltFldVal.split("[#toNumber#]")[1] + "</Value></Leq>");
							}
						}					
					}
				}
			}
			
			//zamkniecie query
			newQuery += MergeCAMLConditions(conditions, 'And');
			newQuery += "</Where>";
		}
	}
	
	//dodanie do newQuery order
	if(newQuery == "<Where></Where>"){
		newQuery = '';
	}
	newQuery += ordQuery;
	
	//dodanie <Query> do newQuery
	newQuery = "<Query>"+newQuery+"</Query>";
	if(ord == false){
		return newQuery;
	}
	else{

	    var doRest = true;

	    if(typeof spQDataCount != 'undefined' && spQDataCount >= 5000){
	        doRest = false;
	        if(clearBtnPress == true){
	            clearBtnPress = false;

	            if(redirUrlOnLoadSpQ(newQuery,1,parseInt($('.ui-pg-selbox').find(":selected").text()),'','',false) === false){
	                doRest = true;
	            }
	        }
	        else{
	            clearBtnPress = false;
	            if(redirUrlOnLoadSpQ(newQuery,1,parseInt($('.ui-pg-selbox').find(":selected").text()),'','',true) === false){
	                doRest = true;
	            }
	        }
	    }

	    if(doRest == true){
	        querySort = true;
	        defQuery = newQuery;
	

	        //pobranie danych pod wyswietlenie
	        gridDataTbl = loadData('');

	        $('#' + gridMarkId).jqGrid('clearGridData');
	        jQuery("#" + gridMarkId).jqGrid('setGridParam',{ 
	            datatype: 'local',
	            data: gridDataTbl,
	        })
            .trigger("reloadGrid");
            gridHyperLinks()
		
	        if(typeof defRowNoChange != 'undefined' && defRowNoChange != 0){
	            var cmpRows = defRowNoChange;
	        }
	        else if(typeof linkedSort != 'undefined' && linkedSort != 0){
	            var cmpRows = linkedSort;
	        }
	        else{
	            var cmpRows = defRowNo;
	        }
	
	        if(gridDataTbl.length >= cmpRows){
	            showNextArr();
	            if(typeof lstPage != 'undefined' && lstPage == true){
	                showNav('last');
	            }
	            else{
	                showNav('');
	            }
				
	        }
	        else{
	            showNav('last');
	        }
	    }
	}
	
}

