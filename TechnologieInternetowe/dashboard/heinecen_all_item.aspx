						if(standShrpSpecMarks(fieldsList[i]) == $node.attr("DisplayName")){
							var nameCheck = "DisplayName";
						}
						else{
							var nameCheck = "StaticName";
						}
						
						//spr czy pole znalezione
						if(standShrpSpecMarks(fieldsList[i]) == $node.attr(nameCheck)){

							//pobranie danych o polach
							if(array_search($node.attr(nameCheck),flds) === false){
								lstGridAll.push(fieldsList[i] + "[lstFld][lstFldName]" + $node.attr("StaticName") + "[/lstFldName][lstFldType]" + $node.attr("Type") + "[/lstFldType][/lstFld]");
