function doSearch(evt,mode,ord){
        
        var currentLoc = window.location.toString().replace("#","?");
        var pathOnly = currentLoc.split('?');       
        var pathToSearch = pathOnly[0].replace('#','') + '?'
        idInUrl = false;

        if(mode != 'srt'){
            totalRecCount = "(?)";
    	}

		var gString = '';
        var elementID = '';
	   
        for (var i = 0; i <  colNamesArray.length; i++) 
		{     
            elementID = 'searchField_' +  colNamesArray[i];     
				 
            if (typeof document.getElementById(elementID) != 'undefined' && document.getElementById(elementID) != null && document.getElementById(elementID).value.trim() != "" && colTypesArray[i]!="flag")
            {        
                var elementValue = document.getElementById(elementID).value.trim();
                if(elementValue.indexOf('SEARCH_DATE') !=- 1)
                {   				   
                    var elementIDdateFrom = 'searchField_dateFrom_' +  colNamesArray[i];     
                    var elementIDdateTo = 'searchField_dateTo_' +  colNamesArray[i];     
					 
                    var dateFrom = document.getElementById(elementIDdateFrom).value.trim();
					var dateTo = document.getElementById(elementIDdateTo).value.trim();
                    if(dateFrom=="" && dateTo=="")
                    {
                        continue;
                    }
                    elementValue = dateFrom + '[#toDate#]' + dateTo;
                }    
                else if(elementValue.indexOf('SEARCH_RANGE') != -1)
                {   				   
                    var elementIDintFrom = 'searchField_intFrom_' +  colNamesArray[i];     
                    var elementIDintTo = 'searchField_intTo_' +  colNamesArray[i];     
					 
                    var intFrom = document.getElementById(elementIDintFrom).value.trim();
                    var intTo = document.getElementById(elementIDintTo).value.trim();
                    if(intFrom =="" && intTo=="")
                    {
                        continue;
                    }
                    elementValue = intFrom + '[#toNumber#]' + intTo;
                } 
                
                gString = gString  + queryStringPrefix +  colNamesArray[i] + "=" + Base64.encode(encodeField(elementValue)) + "&";
                pathToSearch = pathToSearch  + queryStringPrefix +  colNamesArray[i] + "=" + encodeURI(elementValue.replace('&','&amp;').replace("[#toDate#]","_").replace('[#toNumber#]',"_")) + "&";
            }
            else if(colTypesArray[i] == "flag")
            {
                if(document.getElementById(elementID).checked)
                {
                    elementValue="true";
                    gString = gString  + queryStringPrefix +  colNamesArray[i] + "=" + Base64.encode(encodeField(elementValue)) + "&";
                    pathToSearch = pathToSearch  + queryStringPrefix +  colNamesArray[i] + "=" + encodeURI(elementValue.replace('&','&amp;')) + "&";
          
                }                
            }
        }
    
		if(gString.slice(-1) == "&"){
			gString = gString.substring(0,gString.length-1);
		}

		if(mode == "c" || (mode == 's' && gString =='')){
		    clearBtnPress = true;
		}
        
		if (mode == 'e')
		{
		    var rowLimit = getRowLimit();
		    var PAGE_SIZE = 5000000 //rowLimit != null && rowLimit > 100 ? rowLimit : 100;
		    //var pageIndex = 0;
		    var allItemsCount = spQDataCountTotal;
    		    //var allItemsCount = 1501;
		    var pagesCount = 1 //Math.floor(allItemsCount / PAGE_SIZE) + (allItemsCount == PAGE_SIZE ? 0 : 1);

		    pathToSearch += queryStringPrefix + 'export=true';

			$('#myExportLoader').show();
    		    $("#myExportText").html("Work in progress: <span id=\"myExportPercent\">0</span>%");
    		    $("#myExportText").show();

    		    ExportData(pathToSearch, 1, pagesCount, "", 0, PAGE_SIZE);
    	}
		else if(mode == 'srt' || ord == false){
			return execQuery(gString,ord);
		}
		else{
			if(mode == 's'){
				execQuery(gString,ord);
			}
			else{
				appendFltrValues('');
				doSearch(evt,"s",true);
			}
		}
		
		
		if(typeof evt != 'undefined' && evt != ''){
			evt.preventDefault();
		}
}

