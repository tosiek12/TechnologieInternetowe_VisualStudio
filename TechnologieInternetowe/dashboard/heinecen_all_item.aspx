								flds.push($node.attr(nameCheck));
								break;
							}
						}
					}												
				});
			}
		});		
		prepareFields(sortData(fieldsList,lstGridAll));
	}
}

function sortData(fieldsList,lstGridAll){
	
	var resArr = new Array();
	
	if(fieldsList.length != 0){
		if(lstGridAll.length != 0){
		
			for(var i=0;i<fieldsList.length;i++){
				for(var m=0;m<lstGridAll.length;m++){
					if(lstGridAll[m].substring(0,lstGridAll[m].indexOf("[lstFld]")).trim() == fieldsList[i]){
						resArr.push(lstGridAll[m]);
						break;
					}
				}
			}
		}
	}
	return resArr;	
}


