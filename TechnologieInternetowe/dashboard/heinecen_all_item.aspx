function MergeCAMLConditions(conditions, type){

	//enumerator and i or
	var MergeType = {Or : "OR", And: "And"};
	
    // Nie ma nic - powrot
    if (conditions.length == 0){
		return "";
	}
	else{
 
		// Poczatek koniec end
		if(typeof type != 'undefined' && type != '' && type == MergeType.And){var typeStart = "<And>"; var typeEnd = "</And>";}
		else{var typeStart = "<Or>"; var typeEnd = "</Or>";}	
 
		// Budowanie struktury
		while (conditions.length >= 2){
			complexConditions = new Array();
			for (var i = 0; i < conditions.length; i+=2){
				// Tylko jeden warunek zostal
				if (conditions.length == i + 1){
					complexConditions.push(conditions[i]);
				}
				// Zlaczecznie dwoch warunkow
				else{
					complexConditions.push(typeStart + conditions[i] + conditions[i + 1] + typeEnd);
				}
			}
			conditions = complexConditions;
		}
		return conditions[0];
	}
}

function combineQueries(query){
	
	var tmpQ; var tmpQ2; var ordQ;
	var startTags = "<Query><Where>";
	var endTags = "</Where></Query>";
	
	var startTag = "<Query>";
	var endTag = "</Query>";

	var whStartTag = "<Where>";
	var whEndTag = "</Where>";
	var remInd = false;
	var retQuery;

	if(query.indexOf(whStartTag) != -1){
	    query = query.replace(orderByStMk,"<OrderBy>");
	}
	
	if(typeof camlQuery != 'undefined' && camlQuery != '' && camlQuery != "<Query><Where></Where></Query>"){

	    if(camlQuery.indexOf(whStartTag) != -1 || query.indexOf(whStartTag) != -1){
	        remInd = true;
	    }
		tmpQ = camlQuery.replace(startTags,"").replace(endTags,"").replace(startTag,"").replace(endTag,"").trim();
		
		tmpQ2 = query;
		if(tmpQ2.indexOf("<OrderBy") != -1){
		    ordQ = tmpQ2.substring(tmpQ2.indexOf("<OrderBy"),tmpQ2.indexOf("</OrderBy>")+10).trim();
			tmpQ2 = tmpQ2.replace(ordQ,"").trim();
		}
		else{
			ordQ = '';
		}
		
		if(tmpQ2 != "<Query></Query>"){
		    if(tmpQ2.indexOf(tmpQ) == -1){
		        tmpQ = "<And>" + tmpQ;
		        tmpQ2 = tmpQ2.replace(startTags,"").replace(endTags,"").replace(startTag,"").replace(endTag,"").trim();
		        tmpQ += tmpQ2 + "</And>";
            }
		}

		if(remInd == true){
		    ordQ = ordQ.replace(orderByStMk,"<OrderBy>");
		}

		if(ordQ != ''){
		    retQuery = startTags + tmpQ + whEndTag + ordQ + endTag;
		}
		else{
		    retQuery = startTags + tmpQ + endTags;
		}
	}
	else{

	    if(query.indexOf(whStartTag) != -1){
	        retQuery = query.replace(orderByStMk,"<OrderBy>");
	    }
	    else{
	        retQuery = query;
	    }
	}

    if(retQuery.indexOf(whStartTag) == -1 && retQuery.indexOf("<OrderBy>") != -1 && retQuery.indexOf("UseIndexForOrderBy") == -1){
        retQuery = retQuery.replace("<OrderBy>",orderByStMk);
    }
    return retQuery;
	
}

