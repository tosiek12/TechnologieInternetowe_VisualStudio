function prepSortType(srt){
	
	var ready;
	if(srt == "Float" || srt == "float"){
		ready = "float";
	}
	else if(srt == "DateTime" || srt == "date" || srt=="dateonly"){
		ready = "date";
	}
	else if(srt == "Range" || srt == "range"){
	    ready = "range";
	}
	else if(srt == "Text" || srt == "text"){
	    ready = "text";
	}
	else if(srt == "Counter" || srt == "counter" || srt == "number"){
	    ready = "number";
	}
	else if(srt == "Integer" || srt == "integer" || srt == "int"){
	    ready = "integer";
	}
	else{
	    ready = "text";
	}	

	return ready;
}

