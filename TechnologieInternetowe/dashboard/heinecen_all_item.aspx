function changePagerInfo(ster){

    if(typeof totalRecCount != 'undefined' && totalRecCount != '(?)'){
        if(typeof defRowNoChange != 'undefined' && defRowNoChange != 0){
            totalPageCount = Math.ceil(totalRecCount / defRowNoChange);
        }
        else{
            totalPageCount = Math.ceil(totalRecCount / defRowNo);
        }
    }
    else{
        totalRecCount = '(?)'
        totalPageCount = "(?)";
    }

	var toRows = (Page * defRowNo);
	if(typeof totalRecCount != 'undefined' && totalRecCount != '(?)'){var viewEnd = " of " + totalRecCount;}else{var viewEnd = ''}
	if(typeof totalPageCount != 'undefined' && totalPageCount != '(?)'){var viewPage = " of " + totalPageCount;}else{var viewPage = ''}
	
	if(typeof rowsReturned != 'undefined' && rowsReturned != 0){
		if(parseInt(defRowNo) > parseInt(rowsReturned)){
		    toRows = (Page * parseInt(defRowNo)) - (parseInt(defRowNo) - parseInt(rowsReturned));
		}
	}

	$("#" + gridMarkPagerId + " .ui-pg-input").parent().html("Page " + parseInt(Page) + viewPage + " " + "<span class='ui-pg-input' style='display:none'></span>");
	
	if(ster == 'last'){
		$("#" + gridMarkPagerId + " .ui-paging-info").text("View " + ((parseInt(Page) * parseInt(defRowNo)) - parseInt(defRowNo)+1) + " - " + toRows + viewEnd);
	}
	else{
	    $("#" + gridMarkPagerId + " .ui-paging-info").text("View " + ((parseInt(Page) * parseInt(defRowNo)) - parseInt(defRowNo)+1) + " - " + toRows + viewEnd);
	}
	
}

