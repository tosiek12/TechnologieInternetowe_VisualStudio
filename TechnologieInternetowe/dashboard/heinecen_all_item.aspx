function parseUrlOnLoad(){
	var urlParams = parseUrl(); 
    var tmpQ;

    var startTag = "<Query>";
    var endTag = "</Query>";
	
    var whStartTag = "<Where>";
    var whEndTag = "</Where>";
    
	if(urlParams.length != 0){
		if(urlParams['pageGrp'] && urlParams['pageGrp'].length != 0){
			groupByField = urlParams['pageGrp'].toString();
		}	
		if(urlParams['numberOfRows'] && urlParams['numberOfRows'].length != 0){
			linkedSort = parseInt(urlParams['numberOfRows']);
		}
		if(urlParams['queryStr'] && urlParams['queryStr'].length != 0){
			tmpQ = urlParams['queryStr'].toString();
		}
        
		if(typeof tmpQ != 'undefined' && tmpQ != ''){
            
		    var camlQueryTest = camlQuery.replace(startTag,"").replace(endTag,"").replace(whStartTag,"").replace(whEndTag,"").trim();
		    if(camlQueryTest != ""){
		        if(tmpQ.indexOf(camlQueryTest) != -1){
		            tmpQ = tmpQ.replace(camlQueryTest,"");
		        }
		    }

		    var tmpQ3 = tmpQ;
		    if(tmpQ3.indexOf("<OrderBy") != -1){
		        var srtInfo = tmpQ3.substring(tmpQ3.indexOf("<OrderBy"),tmpQ3.indexOf("</OrderBy>")+10).trim();
		        tmpQ3 = tmpQ3.replace(srtInfo,"").replace(startTag,"").replace(endTag,"").replace(whStartTag,"").replace(whEndTag,"").trim();
		        if(tmpQ3.length != 0){
		            loadQuery = tmpQ;
		        }
		    }
		}
		

		//parse sort data
	    if(typeof tmpQ != 'undefined' && tmpQ != '' && tmpQ.indexOf("<OrderBy") != -1 && tmpQ.indexOf("</OrderBy>") != -1 && typeof groupByField != 'undefined' && groupByField != ''){
	        var tmpGr = tmpQ.substring(tmpQ.indexOf("<OrderBy"),tmpQ.indexOf("</OrderBy>")+10).trim();
