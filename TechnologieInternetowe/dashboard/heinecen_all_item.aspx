function parseUrl(pageNr,prevPage,presPage,next,defRowNo,query,lstPage,sortGrdPrmSet,groupByField){
	
    var retArr = new Array(3);
    var urlLoc = decodeURIComponent(window.location.href).toString().replace("?","#").replace("#PageView=","?PageView=").replace("#cvsp_SC_WF_StatusLookup","?cvsp_SC_WF_StatusLookup");
    if(idInUrl == true){
        urlLoc = urlLoc.replace("#IdString=","?IdString=");
    }
    else{
        if(urlLoc.indexOf("#IdString=") != -1){
            var tmpUrlLoc = urlLoc.substring(urlLoc.indexOf("#IdString=")+1,urlLoc.length);
            tmpUrlLoc = tmpUrlLoc.substring(0,tmpUrlLoc.indexOf("#"));
            urlLoc = urlLoc.replace(tmpUrlLoc,"").replace("##","#");
        }
    }
    if(urlLoc.indexOf("#") != -1){
        var myParam = urlLoc.split("#");
    }
    else{
        if(urlLoc.indexOf("?PageView") == -1){
            var myParam = urlLoc.split("?");
        }
        else{
            var myParam = urlLoc.split("#");
        }
    }
   
	if((typeof next == 'undefined' || next == 'undefined' || next == '') && (typeof lstPage == 'undefined' || lstPage == false)){
	    if(myParam.length != 0){
			for(var i=1;i<myParam.length;i++){
			    if(myParam[i].indexOf(trackJq + "true") != -1){
					var paramArr = new Array();
					if(myParam[i].indexOf("&") != -1){
						paramArr = myParam[i].split("&");
					}
					else{
						paramArr[0] = myParam[i];
					}
				
					if(paramArr.length != 0){
						for(var z=0;z<paramArr.length;z++){
							if(paramArr[z].indexOf(pageNo) != -1){
								retArr['pageNo'] = paramArr[z].split("=")[1];
							}
							if(paramArr[z].indexOf(presStr) != -1){
								retArr['presPage'] = Base64.decode(paramArr[z].replace(presStr,""));
							}							
							if(paramArr[z].indexOf(prevStr) != -1){
								retArr['prevPage'] = Base64.decode(paramArr[z].replace(prevStr,""));
							}								
							if(paramArr[z].indexOf(nextStr) != -1){
								retArr['nextStr'] = Base64.decode(paramArr[z].replace(nextStr,""));
							}
							if(paramArr[z].indexOf(sortGrdPrm) != -1){
								retArr['pageSrt'] = Base64.decode(paramArr[z].replace(sortGrdPrm,""));
							}	
							if(paramArr[z].indexOf(groupGrdPrm) != -1){
								retArr['pageGrp'] = Base64.decode(paramArr[z].replace(groupGrdPrm,""));
							}								
							if(paramArr[z].indexOf(queryStr) != -1){
								retArr['queryStr'] = Base64.decode(paramArr[z].replace(queryStr,""));
							}			
							if(paramArr[z].indexOf(numberOfRows) != -1){
								retArr['numberOfRows'] = paramArr[z].split("=")[1];
							}	
						}
					}
					break;
				}
			}
		}
		return retArr;
	}
	else{
		if(myParam.length != 0){
			for(var i=1;i<myParam.length;i++){
				if(myParam[i].indexOf(trackJq + "true") != -1){
					myParam[i] = "";
					break;
				}
			}
		}	
		
		var newUrl = myParam.join("#") + "#" + trackJq + "true";
		newUrl = newUrl.replace("##","#");
		
		if(typeof prevPage != 'undefined' && prevPage != ''){
			newUrl += "&" + prevStr + Base64.encode(prevPage);
		}					
		if(typeof presPage != 'undefined' && presPage != ''){
			newUrl += "&" + presStr + Base64.encode(presPage);
		}			
		if(typeof next != 'undefined' && next != '' && next.length != 0){
			newUrl += "&" + nextStr + Base64.encode(next);
		}
		if(typeof query != 'undefined' && query != '' && query.length != 0){
			newUrl += "&" + queryStr + Base64.encode(query);
		}	
		if(typeof sortGrdPrmSet != 'undefined' && sortGrdPrmSet != '' && sortGrdPrmSet.length != 0){
			newUrl += "&" + sortGrdPrm + Base64.encode(sortGrdPrmSet);
		}
		if(typeof groupByField != 'undefined' && groupByField != '' && groupByField.length != 0){
			newUrl += "&" + groupGrdPrm + Base64.encode(groupByField);
		}
		if(typeof pageNr != 'undefined' && pageNr != ''){
			newUrl += "&" + pageNo + pageNr;
		}	
		if(typeof defRowNo != 'undefined' && defRowNo != ''){
			newUrl += "&" + numberOfRows + defRowNo;
		}		
	    //if(urlLoc.indexOf("cvsp_SC_WF_StatusLookup") == -1){
	    newUrl = newUrl.replace("T&amp;E","T%26amp%3BE");
	    if(urlLoc.indexOf("?IdString=") == -1 && urlLoc.indexOf("cvsp_SC_WF_StatusLookup") == -1){
            window.location.href = newUrl + "&";		
        }
	    //}
	}
}

